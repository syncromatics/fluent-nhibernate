    <PackageReference Include="NHibernate.Driver.FirebirdClient" Version="5.0.0-ci-00140-dbg-NH-3807" />
    <PackageReference Include="NHibernate.Driver.MySql" Version="5.0.0-ci-00140-dbg-NH-3807" />
    <PackageReference Include="NHibernate.Driver.Npgsql" Version="5.0.0-ci-00140-dbg-NH-3807" />
    <PackageReference Include="NHibernate.Driver.SqlServer" Version="5.0.0-ci-00140-dbg-NH-3807" />
    <PackageReference Include="NHibernate.Driver.SqlServer.Compact" Version="5.0.0-ci-00140-dbg-NH-3807" />
    <PackageReference Include="System.Data.SqlClient" Version="4.4.2" />
  </ItemGroup>
</Project>
